{
  "name": "@crosspost/sdk",
  "version": "0.1.0",
  "description": "A TypeScript SDK for interacting with the Crosspost API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "crosspost",
    "social media",
    "api",
    "sdk",
    "twitter",
    "near"
  ],
  "author": "crosspost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crosspost/twitter-proxy"
  },
  "bugs": {
    "url": "https://github.com/crosspost/twitter-proxy/issues"
  },
  "homepage": "https://github.com/crosspost/twitter-proxy#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  }
}
